{
  "name": "mydashboard-utils",
  "version": "2.0.4",
  "description": "Biblioteca de utilidades e constantes myDashboard",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "scripts": {
    "build:esbuild": "node esbuild.config.mjs",
    "build": "npm run build:esbuild",
    "prepare": "npm run build",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."    
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "import": "./dist/*.mjs",
      "require": "./dist/*.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ricardoalvarenga101/mydashboard-utils.git"
  },
  "author": "Ricardo Alvarenga",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ricardoalvarenga101/mydashboard-utils/issues"
  },
  "homepage": "https://github.com/ricardoalvarenga101/mydashboard-utils#readme",
  "dependencies": {
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.45"
  },
  "devDependencies": {
    "esbuild": "^0.27.1",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
